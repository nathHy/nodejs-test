{
  "extends": ["airbnb-base","plugin:jest/recommended"],
  "plugins": ["jest"],
  "rules": {
    "global-require": [0],
    "one-var": [0],
    "one-var-declaration-per-line": [0],
    "no-console": [0],
    "object-curly-newline": [
      0
    ],
    "arrow-parens": [
      "error",
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ]
  }
}